<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    android:background="@drawable/rounded_corners_of_ip_setting_outer_box"
    android:orientation="vertical" >
    <TextView
        android:id="@+id/ws_selectFile_empty_hintLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginTop="30dp"
        android:gravity="center_horizontal"
        android:textColor="#000000"
        android:textSize="20sp" />
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="20dp"
        android:layout_marginTop="30dp" >
        <Button
            android:id="@+id/ws_selectFile_empty_confirm"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="20dp"
            android:layout_weight=".5"
            android:background="@drawable/button_bg_blue_bound"
            android:paddingBottom="10dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="10dp"
            android:text="@string/ip_setting_button_save"
            android:textColor="@color/button_text_color_blue"
            android:textSize="16sp" />
        <Button
            android:id="@+id/ws_selectFile_empty_cannel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_weight=".5"
            android:background="@drawable/button_bg_blue_bound"
            android:paddingBottom="10dp"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingTop="10dp"
            android:text="@string/ip_setting_button_cancel"
            android:textColor="@color/button_text_color_blue"
            android:textSize="16sp" />
    </LinearLayout>
</LinearLayout>